{
  "short_name": "Alor Trader Diary",
  "name": "Alor Trader Diary",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "logo192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "logo512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "intent_filters": {
    "scope_url_scheme": "https",
    "scope_url_host": "maksim-zakharov.github.io",
    "scope_url_path": "/alor-trader-diary"
  },
  "scope": "/alor-trader-diary/#",
  "handle_links": "preferred",
  "start_url": "/diary",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "theme_color": "#000000",
  "background_color": "#000000",
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://maksim-zakharov.github.io/alor-trader-diary/manifest.json"
    }
  ],
  "capture_links": "existing_client_event",
  "shortcuts": [
    {
      "name": "Вывести средства",
      "url": "/diary?drawer=payout"
    },
    {
      "name": "Операции",
      "url": "/diary?drawer=operations"
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "url_handlers": [
    {
      "origin": "https://maksim-zakharov.github.io/alor-trader-diary"
    }
  ],
  "share_target": {
    "action": "/?share_target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "image",
          "accept": ["image/*"]
        }
      ]
    }
  }
}
